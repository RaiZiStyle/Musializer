{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "preLaunchTask": "Build Via .sh (Not all Warning)",
      "args": [],
      "environment": [
        {
          "name": "DISPLAY",
          "value": "localhost:10.0"
        }
      ],
      "stopAtEntry": false,
      "externalConsole": false,
      // "cwd": "/mnt/c/Users/arthu/Documents/01.DEV/01.C/01.PROJET/Olive.c",
      "cwd": "${workspaceFolder}",
      // "program": "/mnt/c/Users/arthu/Documents/01.DEV/01.C/01.PROJET/Olive.c/build/Debug/outDebug",
      "program": "${workspaceFolder}/musializer",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}